{
	"name": "df_dimensao_vendedor",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_data_lake_data_flows_dev_1",
						"type": "DatasetReference"
					},
					"name": "DimVendedor"
				},
				{
					"dataset": {
						"referenceName": "ds_data_lake_csv_generico",
						"type": "DatasetReference"
					},
					"name": "DadosGeograficos"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {COD_VENDEDOR'|'NOME_COMPLETO'|'CARGO'|'REGIAO_VENDA'|'CIDADE_ATUACAO'|'COD_SUP_1'|'NOME_COMP_1'|'COD_SUP_2'|'NOME_COMP_2'|'COD_SUP_3'|'NOME_COMP_3'|'COD_SUP_4'|'NOME_COMP_4'|'COD_SUP_5'|'NOME_COMP_5} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> DimVendedor",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> DadosGeograficos",
				"DimVendedor, DadosGeograficos join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}