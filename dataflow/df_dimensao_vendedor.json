{
	"name": "df_dimensao_vendedor",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_data_lake_data_flows_dev_1",
						"type": "DatasetReference"
					},
					"name": "DimVendedor"
				},
				{
					"dataset": {
						"referenceName": "ds_data_lake_data_flows_dev_2",
						"type": "DatasetReference"
					},
					"name": "DadosGeograficos"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join"
				},
				{
					"name": "select"
				}
			],
			"scriptLines": [
				"source(output(",
				"          COD_VENDEDOR as integer,",
				"          NOME_COMPLETO as string,",
				"          CARGO as string,",
				"          REGIAO_VENDA as string,",
				"          CIDADE_ATUACAO as integer,",
				"          COD_SUP_1 as integer,",
				"          NOME_COMP_1 as string,",
				"          COD_SUP_2 as integer,",
				"          NOME_COMP_2 as string,",
				"          COD_SUP_3 as integer,",
				"          NOME_COMP_3 as string,",
				"          COD_SUP_4 as integer,",
				"          NOME_COMP_4 as string,",
				"          COD_SUP_5 as integer,",
				"          NOME_COMP_5 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> DimVendedor",
				"source(output(",
				"          COD_CIDADE as integer,",
				"          CIDADE as string,",
				"          ESTADO as string,",
				"          PAIS as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> DadosGeograficos",
				"DimVendedor, DadosGeograficos join(CIDADE_ATUACAO == COD_CIDADE,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join",
				"join select(mapColumn(",
				"          COD_VENDEDOR,",
				"          NOME_COMPLETO,",
				"          CARGO,",
				"          REGIAO_VENDA,",
				"          CIDADE_ATUACAO = CIDADE,",
				"          COD_SUP_1,",
				"          NOME_COMP_1,",
				"          COD_SUP_2,",
				"          NOME_COMP_2,",
				"          COD_SUP_3,",
				"          NOME_COMP_3,",
				"          COD_SUP_4,",
				"          NOME_COMP_4,",
				"          COD_SUP_5,",
				"          NOME_COMP_5",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select"
			]
		}
	}
}