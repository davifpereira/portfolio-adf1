{
	"name": "df_dimensao_vendedor",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_data_lake_csv_generico",
						"type": "DatasetReference"
					},
					"name": "DimVendedor"
				},
				{
					"dataset": {
						"referenceName": "ds_data_lake_csv_generico",
						"type": "DatasetReference"
					},
					"name": "DadosGeograficos"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimVendedor",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DadosGeograficos",
				"DimVendedor, DadosGeograficos join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}